{
  "name": "curatedstack-nocode-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "analyze": "babel-node scripts/analyze.js",
    "get-logos": "babel-node scripts/getLogos.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/lunr-module": "^0.3.1",
    "@nuxtjs/pwa": "^3.0.2",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "nuxt-floating-prompt-module": "^1.0.0",
    "nuxt-i18n": "^6.15.4",
    "nuxtjs-darkmode-js-module": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^4.1.10",
    "download": "^8.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-metadata": "^1.7.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "logo-scrape": "^1.1.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.3",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
